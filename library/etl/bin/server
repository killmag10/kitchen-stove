#!/bin/bash

# Get the base dir path.
BASE_DIR=`printf '%s' "$BASH_SOURCE" | sed 's#/\?[^/]*$##' | sed 's#^./##'`;
if [[ ! "$BASE_DIR" =~ ^/ ]]; then
    BASE_DIR=`printf '%s' "$PWD/$BASE_DIR" | sed 's#/\.\?$##'`;
fi
. "$BASE_DIR/include/environment.sh"

options=''
optionsLong=''
res=`getopt -o "$options" -l "$optionsLong" -- "$@"` || exit 1;
eval 'set -- '"$res"

# Define options vars.
optionType=''
optionLogLevel='Basic'

# Read options.
param="$1"
while shift; do
case "$param" in
    --)
        break;
        ;;
esac
param="$1"
done

# Get IP and port.
ip="$1";
port="$2";
shift 2

cd "$ETL_KETTLE_DIR_APPLICATION"
./carte.sh "$ip" "$port"
exit $?
